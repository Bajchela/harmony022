        </div>
    </section>

</body>

</html>
<script>
    $(window).scroll(function () {
        var scroll = $(window).scrollTop();
        if ((scroll > 0) && (scroll < 50)) {
            $('#logoSlika').width(200);
        }
        else if (scroll > 50) {
            $('#logoSlika').width(100);
        }
    })
</script>