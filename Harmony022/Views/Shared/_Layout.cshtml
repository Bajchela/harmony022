                                    <li>@Html.ActionLink("Prodaja", "showBSSell", "vidiNekretnine")</li>
                                    <li>@Html.ActionLink("Izdavanje", "showBSRent", "vidiNekretnine")</li>

                                </ul>
                            </li>
                            <li class="dropdown-submenu">
                                <a href="#">Poljoprivredno zemljište</a>
                                <ul class="dropdown-menu">
                                    <li>@Html.ActionLink("Prodaja", "showLandSell", "vidiNekretnine")</li>
                                    <li>@Html.ActionLink("Izdavanje", "showLandRent", "vidiNekretnine")</li>

                                </ul>
                            </li>
                            <li class="dropdown-submenu">
                                <a href="#">Građevinsko zemljište</a>
                                <ul class="dropdown-menu">
                                    <li>@Html.ActionLink("Prodaja", "showConstructionLandSell", "vidiNekretnine")</li>
                                    <li>@Html.ActionLink("Izdavanje", "showConstructionLandRent", "vidiNekretnine")</li>

                                </ul>
                            </li>
                            <li class="nav-divider"></li>

                        </ul>
                    </li>

                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            Legalizacija <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li class="dropdown-submenu">
                            <li>@Html.ActionLink("Geometar", "prikaziGeometarStranicu", "Home")</li>
                            <li>@Html.ActionLink("Projektant", "prikaziProjektantStranicu", "Home")</li>
                        </ul>

                    </li>

                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            Katastar nepokretnosti<span class="caret"></span>
                        </a>

                        <ul class="dropdown-menu">
                            <li class="dropdown-submenu">
                                <a>Sve vrsta usluga u katastru</a>
                                <ul class="dropdown-menu">
                                    <li>
                                        <a>Uknjižba</a>
                                    </li>
                                    <li>
                                        <a>Vlasnički list</a>
                                    </li>
                                    <li>
                                        <a>Kopija plana</a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </li>


                    <li>@Html.ActionLink("O nama", "prikaziONamaStranicu", "Home")</li>
                    <li>@Html.ActionLink("Kontakt", "Contact", "Home")</li>


                </ul>
                <ul class="nav navbar-nav navbar-right">
                    @if (@User.Identity.Name != "")
                    {
                        <li>@Html.ActionLink(" Admin panel  ", "prikaziGlavnuStranicu", "Admin")</li>
                    }
                    @Html.Partial("_LoginPartial")
                </ul>

            </div>
        </div>
    </div>

    <div class="container body-content" style="margin-top:110px">
        @RenderBody()


    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)


