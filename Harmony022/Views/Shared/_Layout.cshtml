<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Harmony022 - Agencija za nekretnine</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Abril+Fatface" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <link rel="stylesheet" href="~/Content/footer/bootstrap-grid.css">
    
    <style>
        
        li.dropdown-submenu:hover ul {
            display: block;
            left: 100%;
        }

        .dropdown-submenu {
            position: relative;
        }

            .dropdown-submenu:hover .dropdown-menu {
                top: 0;
            }

        #footer-section {
            background-color: rgba(20, 20, 20, 0.904);
        }

            #footer-section h6 {
                color: white;
            }

        tbody th {
            padding: 0px;
        }

        #col-margin {
        }

        #footer-end {
            background-color: rgba(27, 32, 32, 0.856);
        }

        #logo-fb {
            width: 46px;
            margin-left: 20px;
        }

        #logo-insta {
            margin-left: 20px;
            width: 46px;
        }

        #logo-gmp {
            width: 86px;
            margin-bottom: 5px;
            margin-left: 3px;
        }

        .img-fluid {
            width: 20vw;
        }
    </style>
</head>
<body style=" padding-bottom: 0px;background-color: white;padding-top: 110px;">
 

    <div class="navbar navbar-inverse navbar-fixed-top " style="background-color:white">
      
        <div class="container-fluid">
            <div class="row text-center">
                <div class="col-12">
                    <img id="logoSlika" src="~/Content/Harmony022.png" style="width: 200px;" alt="">
                </div>

            </div>
        </div>
        <div>

            <div class="col-12 ">
                <!--<img class="col-2 "style="float:unset;text-align:center" src="~/Content/Nekretnine/logo.jpg"/>-->
            </div>
        </div>
        <div class="container">

            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Harmony022", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>

            <div class="navbar-collapse collapse text-center">
                <ul class="nav navbar-nav">
                    <li>

                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            Nekretnine <span class="caret"></span>
                        </a>

                        <ul class="dropdown-menu">
                            <li class="dropdown-submenu">
                                <a href="#">Stanovi</a>
                                <ul class="dropdown-menu">
                                    <li>@Html.ActionLink("Prodaja", "prikaziStanoveProdaja", "vidiNekretnine")</li>
                                    <li>@Html.ActionLink("Izdavanje", "prikaziStanoveRenta", "vidiNekretnine")</li>

                                </ul>
                            </li>
                            <li class="dropdown-submenu">
                                <a href="#">Kuće</a>
                                <ul class="dropdown-menu">
                                    <li>@Html.ActionLink("Prodaja", "showHomeSell", "vidiNekretnine")</li>
                                    <li>@Html.ActionLink("Izdavanje", "showHomeRent", "vidiNekretnine")</li>

                                </ul>
                            </li>
                            <li class="dropdown-submenu">
                                <a href="#">Vikendice</a>
                                <ul class="dropdown-menu">
                                    <li>@Html.ActionLink("Prodaja", "showCottageSell", "vidiNekretnine")</li>
                                    <li>@Html.ActionLink("Izdavanje", "showCottageRent", "vidiNekretnine")</li>

                                </ul>
                            </li>

                            <li class="dropdown-submenu">
                                <a href="#">Poslovni prostor</a>
